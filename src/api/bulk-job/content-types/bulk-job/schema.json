{
  "kind": "collectionType",
  "collectionName": "bulk_jobs",
  "info": {
    "singularName": "bulk-job",
    "pluralName": "bulk-jobs",
    "displayName": "Bulk Job",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "jobId": {
      "type": "uid",
      "unique": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "queued",
        "processing",
        "completed",
        "failed"
      ],
      "default": "pending"
    },
    "total_items": {
      "type": "integer"
    },
    "processed_items": {
      "type": "integer",
      "default": 0
    },
    "error": {
      "type": "text"
    },
    "meta": {
      "type": "json"
    },
    "payload": {
      "type": "json",
      "private": true
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "bulk_jobs"
    },
    "deviceId": {
      "type": "string"
    },
    "uploadSessionId": {
      "type": "string"
    }
  }
}
